<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="co.com.smartfit.web.entities.PlanesEmpresa" table="planes_por_empresa" optimistic-lock="version">
        <id name="id" type="java.lang.String">
            <column name="id" unique="true" not-null="true"/>
            <generator class="identity"/>
        </id>
        <property name="idEmpresa" type="java.lang.Integer">
            <column name="idEmpresa"/>
        </property>
        <property name="nombreEmpresa" type="java.lang.String">
            <column name="nombreEmpresa"/>
        </property>
        <property name="idPlan" type="java.lang.Integer">
            <column name="idPlan"/>
        </property>
        <property name="nombrePlan" type="java.lang.String">
            <column name="nombrePlan"/>
        </property>
    </class>
</hibernate-mapping>
